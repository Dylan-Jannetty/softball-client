<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">Leaderboards</div>
  <!-- Table -->
  <table class="all-teams table">
    <tr>
      <th>Rank</th>
      <th>Name</th>
      <th>Wins</th>
      <th>Losses</th>
    </tr>
    {{#each teams as |team|}}
    <tr class="team-ranks">
      <td class="rank">{{team.rank}}</td>
      <td class="team-name"><a href="#" data-team-id="{{team.id}}">{{team.name}}</a></td>
      <td class="team-wins">{{team.win}}</td>
      <td class="team-losses">{{team.loss}}</td>
    </tr>
    {{/each}}

  </table>
</div>
